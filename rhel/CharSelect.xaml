<Window x:Class="rhel.CharSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Character Select" ResizeMode="NoResize" Icon="/rhel;component/rhel.ico" mc:Ignorable="d" WindowStartupLocation="CenterScreen" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="345" Width="350">
    <Grid>
        <ScrollViewer Name="characterView" Margin="0,0,0,42">
            <StackPanel Name="CharacterPanel" Margin="0" />
        </ScrollViewer>
        <Button Content="Save" Name="save" Height="23" Width="97" Margin="12,271,0,12" HorizontalAlignment="Right" />
        <Button Content="Back" Name="cancel" Height="23" Width="97" Click="cancel_Click" Margin="12,271,0,12" HorizontalAlignment="Left" />
    </Grid>
</Window>
